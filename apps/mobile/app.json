{
  "expo": {
    "name": "Chainlens",
    "slug": "chainlens-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "owner": "epsilon",
    "scheme": "chainlensmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Chainlens is a fully open source AI assistant that helps you accomplish real-world tasks with ease. Through natural conversation, Chainlens becomes your digital companion for research, data analysis, and everyday challenges.",
    "primaryColor": "#121215",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "keywords": [
      "AI",
      "artificial intelligence",
      "browser automation",
      "web scraping",
      "file management",
      "AI assistant",
      "open source",
      "research",
      "data analysis"
    ],
    "githubUrl": "https://github.com/epsilon-ai/chainlens",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.epsilon.chainlens",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "CFBundleDisplayName": "Chainlens",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.epsilon.chainlens",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "CAMERA",
        "RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "name": "Chainlens - Open Source Generalist AI Agent",
      "shortName": "Chainlens",
      "description": "Chainlens is a fully open source AI assistant that helps you accomplish real-world tasks with ease through natural conversation.",
      "startUrl": "/",
      "display": "standalone",
      "orientation": "portrait",
      "themeColor": "#121215",
      "backgroundColor": "#121215",
      "lang": "en",
      "scope": "/",
      "crossorigin": "use-credentials"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/epsilon-splash.png",
          "imageWidth": 400,
          "resizeMode": "contain",
          "backgroundColor": "#121215"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "metadata": {
        "applicationName": "Chainlens",
        "authors": "Epsilon Team",
        "creator": "Epsilon Team",
        "publisher": "Epsilon Team",
        "category": "Technology",
        "siteUrl": "https://chainlens.net",
        "twitterHandle": "@epsilonai"
      }
    }
  }
}